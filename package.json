{
  "name": "docket-api",
  "version": "0.1.0",
  "description": "Docket API",
  "main": "index.js",
  "scripts": {
    "generate:secret": "node --eval \"console.log(crypto.randomBytes(64).toString('hex'))\"",
    "start": "node index.js",
    "start:dev": "npx nodemon ./index.js",
    "start:prod": "npx pm2 start ./index.js",
    "start:worker": "node ./worker.js",
    "start:debug": "node --inspect=0.0.0.0:9299 ./index.js",
    "start:worker:dev": "npx nodemon ./worker.js",
    "start:worker:prod": "npx pm2 start ./worker.js",
    "start:worker:debug": "node --inspect=0.0.0.0:9299 ./worker.js",
    "start:repl": "node ./repl.js",
    "test": "snyk test && npx npm-run-all test:*",
    "test:unit": "npx jest ./__tests__/unit",
    "lint": "npx eslint ./src",
    "db:seed": "./db-seed.sh",
    "routes": "node ./routes.js",
    "snyk-protect": "snyk protect",
    "cz": "npx git-cz",
    "aws:cloudformation": "node ./aws/cloudformation/create-or-update-stack.js",
    "vault:seed": "node ./vault-seed.js"
  },
  "author": "John Goldsmith <john.goldsmith@docket.fyi>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/docket-fyi/docket-api.git"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.6.0",
    "@hapi/joi": "17.0.2",
    "@microsoft/microsoft-graph-client": "2.0.0",
    "@sentry/node": "5.12.4",
    "accept-language": "3.0.18",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "bull": "3.13.0",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-pino-logger": "4.0.0",
    "googleapis": "47.0.0",
    "http-status": "1.4.2",
    "ioredis": "4.16.0",
    "json-api-serializer": "2.3.0",
    "jsonapi-serializer": "3.6.6",
    "jsonwebtoken": "8.5.1",
    "moment": "2.24.0",
    "node-fetch": "2.6.0",
    "node-vault": "0.9.12",
    "nodemailer": "6.4.2",
    "pg": "7.18.2",
    "pg-hstore": "2.3.3",
    "pm2": "4.2.1",
    "qs": "6.9.3",
    "sequelize": "5.21.5",
    "slugify": "1.3.6",
    "socket.io": "2.3.0",
    "stripe": "8.20.0",
    "swagger-jsdoc": "3.5.0",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.0",
    "autocannon": "4.6.0",
    "aws-sdk": "2.624.0",
    "clinic": "5.0.0",
    "commitizen": "4.0.3",
    "coveralls": "3.0.9",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "express-list-endpoints": "4.0.1",
    "husky": "4.2.3",
    "inquirer": "7.0.4",
    "jest": "24.9.0",
    "jest-junit": "10.0.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "pino-pretty": "3.5.0",
    "semantic-release": "17.0.4",
    "sequelize-cli": "5.5.1",
    "snyk": "1.295.0",
    "yaml-cfn": "0.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test:unit"
    }
  },
  "snyk": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
