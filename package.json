{
  "name": "docket-api",
  "version": "0.1.0",
  "description": "Docket API",
  "main": "index.js",
  "scripts": {
    "generate:secret": "node --eval \"console.log(crypto.randomBytes(64).toString('hex'))\"",
    "start": "node ./index.js",
    "start:dev": "nodemon ./index.js",
    "start:worker": "nodemon ./worker.js",
    "start:prod": "$(npm bin)/pm2 start ./index.js",
    "start:worker:prod": "$(npm bin)/pm2 start ./worker.js",
    "start:debug": "node --inspect=0.0.0.0:9299 ./index.js",
    "start:worker:debug": "node --inspect=0.0.0.0:9299 ./worker.js",
    "start:repl": "node ./repl.js",
    "test": "$(npm bin)/npm-run-all test:*",
    "test:unit": "$(npm bin)/jest ./__tests__/unit",
    "lint": "$(npm bin)/eslint ./src",
    "db:seed": "node ./src/v1/seeds/index.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "author": "John Goldsmith <johngoldsm1@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/docket-fyi/docket-api.git"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^1.5.2",
    "accept-language": "3.0.18",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "bull": "3.7.0",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "googleapis": "38.0.0",
    "http-status": "1.3.1",
    "ioredis": "4.6.2",
    "joi": "14.3.1",
    "jsonwebtoken": "8.5.1",
    "moment": "2.24.0",
    "mongodb": "3.2.2",
    "mongoose": "5.4.18",
    "node-fetch": "2.3.0",
    "nodemailer": "5.1.1",
    "pm2": "3.4.0",
    "qs": "6.7.0",
    "slugify": "1.3.4",
    "socket.io": "2.2.0",
    "snyk": "^1.189.0"
  },
  "devDependencies": {
    "autocannon": "3.2.1",
    "clinic": "4.0.0",
    "coveralls": "3.0.3",
    "eslint": "5.15.3",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "nodemon": "1.18.10",
    "npm-run-all": "4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test:unit"
    }
  },
  "snyk": true
}
